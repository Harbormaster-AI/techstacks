<tech-stack-package 
    name="Base Ruby on Rails Tech Stack"
    shortName=".ruby.tech.stack"
    description="The base for all Ruby on Rails related tech stacks" 
    icon-url=""
 	info-page-url="http://www.harbormaster.ai/.rails.tech.stack"
    version="1.0"
    app-type="webapp"
    release-status="production"
    visible="false"
    derivedFrom=".common.tech.stack">
	<target-languages>
		<target-language name="rails"/>
		<target-language name="javascript"/>
	</target-languages><!-- target language(s) supported by the template structure -->
	<tech-stack>
	    <tech-stack-component name="presentation" description="view templates " app-layer="presentation"/>
	    <tech-stack-component name="build" description="ci/cd templates " app-layer="build"/>
	    <tech-stack-component name="data" description="sqlite" app-layer="data"/>
	</tech-stack>
</tech-stack-package>
