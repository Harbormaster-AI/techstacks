<tech-stack-package 
    name="Go Tech Stack"
    shortName="Golang"
    description="The base for all Go related tech stacks" 
    icon-url="img/go.lang.svg"
 	info-page-url="http://www.harbormaster.ai/.go.tech.stack"
    version="1.0"
    app-type="webapp"
    release-status="production"
    derivedFrom=".common.tech.stack"
    visible="true">
	<target-languages>
		<target-language name="go"/>
		<target-language name="javascript"/>
	</target-languages><!-- target language(s) supported by the template structure -->
	<tech-stack>
	    <tech-stack-component name="build" description="ci/cd templates " app-layer="build"/>
	    <tech-stack-component name="data" description="postgres" app-layer="data"/>
	</tech-stack>
</tech-stack-package>
