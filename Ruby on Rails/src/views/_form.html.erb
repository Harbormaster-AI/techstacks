#set( $className = ${Utils.capitalizeFirstLetter($classObject.getName())} )
#set( $lowercaseClassName = ${Utils.lowercaseFirstLetter( ${className})} )
<%= form_with model: @${lowercaseClassName}, local: true do |form| %>
 
  <% if @${lowercaseClassName}.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@${lowercaseClassName}.errors.count, "error") %> prohibited
        this ${lowercaseClassName} from being saved:
      </h2>
      <ul>
        <% @${lowercaseClassName}.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="card">
    <div class="card-body">

#formFields()

      <%= form.submit %>
    
    </div>
  
  </div> 
<% end %>