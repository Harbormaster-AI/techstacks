<tech-stack-package 
	name="Azure Functions" 
	shortName="AzureFunctions"
	description="Serverless implementation for Azure Cloud" 
	icon-url="./img/azurefunction.png" 	
	info-page-url="https://harbormaster.ai/azure-function-tech-stack/"
    version="0.8"
    app-type="restfulapi"
    release-status="beta"
    derivedFrom=".java.tech.stack,.azure.tech.stack"
	supportsDocker="true"
	supportsKubernetes="true"
	product-line="Serverless"
	visible="false">
	<target-languages>
		<target-language name="java"/>
	</target-languages><!-- target language(s) supported by the template structure -->
	<tech-stack>
		<tech-stack-component name="serverless functions" description="RESTful API web services backend" app-layer="business"></tech-stack-component>
		<tech-stack-component name="noqsl" description="MongoDB" app-layer="data"></tech-stack-component>
		<tech-stack-component name="junit" description="JUnit to test core functionality" app-layer="test"/>	
		<tech-stack-component name="maven" description="build, commit, package, deploy, run module" app-layer="build"/>
	</tech-stack>
</tech-stack-package>
