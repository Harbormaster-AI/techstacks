<tech-stack-package 
	name="CordaMarketplaceToken"
	shortName="CordaMktplace"
	description="Corda Token reference for a market place scenario, using CQRS and persistent option of JPA or MongoDB"
	icon-url="./img/corda.png"
	info-page-url="https://harbormaster.ai/corda-token-tech-stack/"
    version="0.8"
    app-type="restfulapi"
    release-status="beta"
    derivedFrom="Axon4,.corda.token.tech.stack"
	supportsDocker="true"
	supportsKubernetes="true"
	product-line="Blockchain"
	visible="true"
	primary-vendor="r3">
	<target-languages>
		<target-language name="java"/>
		<target-language name="kotlin"/>
	</target-languages><!-- target language(s) supported by the template structure -->
	<tech-stack>
		<tech-stack-component name="spring boot" description="RESTful API web services backend" app-layer="presentation"></tech-stack-component>
		<tech-stack-component name="noqsl" description="MongoDB" app-layer="data"></tech-stack-component>
		<tech-stack-component name="jpa" description="JPA" app-layer="data"></tech-stack-component>
		<tech-stack-component name="junit" description="JUnit to test core functionality" app-layer="test"/>	
		<tech-stack-component name="gradle" description="build, commit, package, deploy, run module" app-layer="build"/>
	</tech-stack>
</tech-stack-package>
