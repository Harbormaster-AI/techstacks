<tech-stack-package 
    name="Django"
    shortName="Django"
    description="Django is a Python-based open-source web framework, which follows the model-view-template architectural pattern" 
    icon-url="./img/django.png"
	info-page-url="http://www.harbormaster.ai/Angular11.tech.stack"
    version="1.0"
    app-type="webapp"
    release-status="production"
    derivedFrom=".python.tech.stack"
	supportsDocker="true"
	supportsKubernetes="true"
	product-line="WebApp">
	<target-languages>
		<target-language name="python"/>
		<target-language name="javascript"/>
		<target-language name="html5"/>
	</target-languages><!-- target language(s) supported by the template structure -->
	<tech-stack>
		<tech-stack-component name="django-ui" description="use the Django web framework" app-layer="presentation"/>
		<tech-stack-component name="django-model" description="use the Django model framework" app-layer="business"/>
		<tech-stack-component name="django-orm" description="use the Django ORM framework" app-layer="data"/>
		<tech-stack-component name="build" description="Python build" app-layer="build"/>
		<tech-stack-component name="test" description="Pytest" app-layer="test"/>
	</tech-stack>
</tech-stack-package>
